#include <iostream>
using namespace std;

int main() {
    int a;
    cin >> a;
        if (a < 2) {
            cout << "N0";
            return 0;
        }
        for (int i = 2; i * i <= a; i++) {
            if (a % i == 0){
                cout << "No";
                return 0;
        }
        }
        cout << "Yes";
    return 0;
}
