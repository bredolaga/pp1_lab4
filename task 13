#include <iostream>
using namespace std;

int main() {
    long long a;
    int b, c;
    c = 0;
    cin >> a >> b;
    long long arrA[a];
    for (int i = 0;i < a;i++) {
        cin >> arrA[i];
    }
    for (int i = 0;arrA[i] < b && i < a;i++) {
        c++;
        if (arrA[i] == b) {
            cout << b;
            return 0;
        }
    }
    cout << c;
    return 0;
}
