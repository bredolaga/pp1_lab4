#include <iostream>
using namespace std;

int main() {
    long long a;
    int b = 0;
    cin >> a;
    long long arrA[a];
    for (int i = 0;i < a;i++) {
        cin >> arrA[i];
    }
    for (int i = 0;i < a;i++) {
        if ((arrA[i] % 10) == 7) {
            b++;
        }
    }
    cout << b;
    return 0;
}
